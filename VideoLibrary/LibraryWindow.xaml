<Window x:Class="VideoLibrary.LibraryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VideoLibrary"
        mc:Ignorable="d"
        Title="LibraryWindow" Height="900" Width="1440">
    <Grid>
        <DockPanel HorizontalAlignment="Left" Height="100" LastChildFill="False" Margin="286,303,0,0" VerticalAlignment="Top" Width="100"/>
        <Frame x:Name="TypeFrame" Content="TypeFrame" HorizontalAlignment="Left" Height="819" Margin="0,50,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.344,0.69" Width="100" BorderThickness="1" BorderBrush="Black"/>
        <Frame x:Name="ContentFrame" Content="ContentFrame" HorizontalAlignment="Left" Height="819" Margin="100,50,0,0" VerticalAlignment="Top" Width="1332" BorderThickness="1" BorderBrush="Black"/>
        <ToolBarTray HorizontalAlignment="Left" Height="50" VerticalAlignment="Top" Width="1432">
            <ToolBar x:Name="toolBar" Margin="0,0,-1432,-50">
                <Button x:Name="importButton" Content="导入" Height="45" VerticalAlignment="Top" Width="75" Click="importButton_Click"/>

            </ToolBar>
        </ToolBarTray>

    </Grid>
</Window>
